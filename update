#!/usr/bin/env bash

set -e

sources=$(cat ./data/sources.txt)

for source in $sources; do
    curl --silent $source >> tmp
done;

cat data/free.txt tmp \
    | sed '/./,$!d' \
    | sed -e 's/^ *//' -e 's/ *$//' \
    | awk '{print tolower($0)}' \
    | sort \
    | uniq \
    | comm -23 - ./data/blacklist.txt \
    | comm -23 - ./data/disposable.txt > ./data/free.txt

echo 'Complete!'
